<base:ProfileIconUserControl x:Class="ProtonVPN.Client.Common.UI.Assets.Icons.Profiles.ShoppingProfileIcon"
                             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                             xmlns:base="using:ProtonVPN.Client.Common.UI.Assets.Icons.Base"
                             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                             xmlns:local="using:ProtonVPN.Client.Common.UI.Assets.Icons.Profiles"
                             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                             x:DefaultBindMode="OneWay"
                             mc:Ignorable="d">

    <UserControl.Resources>

        <LinearGradientBrush x:Key="gradient_brush_white" MappingMode="RelativeToBoundingBox" StartPoint="0,0" EndPoint="1,1">
            <LinearGradientBrush.GradientStops>
                <GradientStopCollection>
                    <GradientStop Color="#A0FFFFFF" />
                    <GradientStop Offset="0.2" Color="#80FFFFFF" />
                    <GradientStop Offset="1" Color="#00FFFFFF" />
                </GradientStopCollection>
            </LinearGradientBrush.GradientStops>
        </LinearGradientBrush>

        <LinearGradientBrush x:Key="gradient_brush_white_2" MappingMode="RelativeToBoundingBox" StartPoint="0.8,0" EndPoint="0.6,1">
            <LinearGradientBrush.GradientStops>
                <GradientStopCollection>
                    <GradientStop Color="#A0FFFFFF" />
                    <GradientStop Offset="1" Color="#00FFFFFF" />
                </GradientStopCollection>
            </LinearGradientBrush.GradientStops>
        </LinearGradientBrush>

        <LinearGradientBrush x:Key="gradient_brush_black" MappingMode="RelativeToBoundingBox" StartPoint="0,0.5" EndPoint="1,0">
            <LinearGradientBrush.GradientStops>
                <GradientStopCollection>
                    <GradientStop Color="#66000000" />
                    <GradientStop Offset="0.3" Color="#00000000" />
                </GradientStopCollection>
            </LinearGradientBrush.GradientStops>
        </LinearGradientBrush>

        <LinearGradientBrush x:Key="gradient_brush_black_2" MappingMode="RelativeToBoundingBox" StartPoint="0,0" EndPoint="0,1">
            <LinearGradientBrush.GradientStops>
                <GradientStopCollection>
                    <GradientStop Offset="0" Color="#11000000" />
                    <GradientStop Offset="1" Color="#66000000" />
                </GradientStopCollection>
            </LinearGradientBrush.GradientStops>
        </LinearGradientBrush>

        <LinearGradientBrush x:Key="gradient_brush_black_3" MappingMode="RelativeToBoundingBox" StartPoint="0,0" EndPoint="1,0">
            <LinearGradientBrush.GradientStops>
                <GradientStopCollection>
                    <GradientStop Offset="0" Color="#00000000" />
                    <GradientStop Offset="1" Color="#33000000" />
                </GradientStopCollection>
            </LinearGradientBrush.GradientStops>
        </LinearGradientBrush>

    </UserControl.Resources>

    <Viewbox Stretch="{x:Bind Stretch}">
        <Canvas Width="36"
                Height="24">

            <Path Data="M8 3H33.6126C34.2951 3 34.7771 3.6687 34.5613 4.31623L31.2279 14.3162C31.0918 14.7246 30.7097 15 30.2792 15H12L8 3Z"
                  Fill="{x:Bind AccentColor, Converter={StaticResource ProfileColorToBrushConverter}}" />
            <Path Data="M8 3H33.6126C34.2951 3 34.7771 3.6687 34.5613 4.31623L31.2279 14.3162C31.0918 14.7246 30.7097 15 30.2792 15H12L8 3Z"
                  Fill="{StaticResource gradient_brush_white_2}" />
            <Path Data="M8 3H33.6126C34.2951 3 34.7771 3.6687 34.5613 4.31623L31.2279 14.3162C31.0918 14.7246 30.7097 15 30.2792 15H12L8 3Z"
                  Fill="{StaticResource gradient_brush_black}" />
            <Path Data="M0 1C0 0.447715 0.447715 0 1 0H6.88235C7.3201 0 7.70697 0.284715 7.83713 0.702666L12.9124 17H29C29.5523 17 30 17.4477 30 18C30 18.5523 29.5523 19 29 19H12.1765C11.7387 19 11.3519 18.7153 11.2217 18.2973L6.1464 2H1C0.447715 2 0 1.55228 0 1Z"
                  Fill="{x:Bind AccentColor, Converter={StaticResource ProfileColorToBrushConverter}}" />
            <Path Data="M0 1C0 0.447715 0.447715 0 1 0H6.88235C7.3201 0 7.70697 0.284715 7.83713 0.702666L12.9124 17H29C29.5523 17 30 17.4477 30 18C30 18.5523 29.5523 19 29 19H12.1765C11.7387 19 11.3519 18.7153 11.2217 18.2973L6.1464 2H1C0.447715 2 0 1.55228 0 1Z"
                  Fill="{StaticResource gradient_brush_white}" />

            <Ellipse Canvas.Left="14"
                     Canvas.Top="20"
                     Width="4"
                     Height="4"
                     Fill="{x:Bind AccentColor, Converter={StaticResource ProfileColorToBrushConverter}}" />
            <Ellipse Canvas.Left="14"
                     Canvas.Top="20"
                     Width="4"
                     Height="4"
                     Fill="{StaticResource gradient_brush_white}" />
            <Ellipse Canvas.Left="24"
                     Canvas.Top="20"
                     Width="4"
                     Height="4"
                     Fill="{x:Bind AccentColor, Converter={StaticResource ProfileColorToBrushConverter}}" />
            <Ellipse Canvas.Left="24"
                     Canvas.Top="20"
                     Width="4"
                     Height="4"
                     Fill="{StaticResource gradient_brush_white}" />

            <Path Data="M12.2171 6.43416C12.1173 6.23469 12.2624 6 12.4854 6H30.5146C30.7376 6 30.8827 6.23469 30.7829 6.43416L30.0829 7.83416C30.0321 7.9358 29.9282 8 29.8146 8H13.1854C13.0718 8 12.9679 7.9358 12.9171 7.83416L12.2171 6.43416Z"
                  Fill="{StaticResource gradient_brush_black_2}" />
            <Path Data="M14.1619 10.4102C14.0842 10.2133 14.2293 10 14.4409 10H28.5591C28.7707 10 28.9158 10.2133 28.8381 10.4101L28.2855 11.8101C28.2402 11.9247 28.1296 12 28.0064 12H14.9936C14.8704 12 14.7598 11.9247 14.7145 11.8101L14.1619 10.4102Z"
                  Fill="{StaticResource gradient_brush_black_2}" />
            <Path Data="M12.2171 6.43416C12.1173 6.23469 12.2624 6 12.4854 6H30.5146C30.7376 6 30.8827 6.23469 30.7829 6.43416L30.0829 7.83416C30.0321 7.9358 29.9282 8 29.8146 8H13.1854C13.0718 8 12.9679 7.9358 12.9171 7.83416L12.2171 6.43416Z"
                  Fill="{StaticResource gradient_brush_black_3}" />
            <Path Data="M14.1619 10.4102C14.0842 10.2133 14.2293 10 14.4409 10H28.5591C28.7707 10 28.9158 10.2133 28.8381 10.4101L28.2855 11.8101C28.2402 11.9247 28.1296 12 28.0064 12H14.9936C14.8704 12 14.7598 11.9247 14.7145 11.8101L14.1619 10.4102Z"
                  Fill="{StaticResource gradient_brush_black_3}" />
        </Canvas>
    </Viewbox>

</base:ProfileIconUserControl>