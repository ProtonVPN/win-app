	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\..\Common\ProtonVPN.Common.Core\ProtonVPN.Common.Core.csproj" />
		<ProjectReference Include="..\ProtonVPN.Serialization.Contracts\ProtonVPN.Serialization.Contracts.csproj" />
	</ItemGroup>
</Project>