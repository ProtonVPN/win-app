	</PropertyGroup>
	<ItemGroup>
		<Compile Include="..\..\GlobalAssemblyInfo.cs" Link="Properties\GlobalAssemblyInfo.cs" />
	</ItemGroup>
	<ItemGroup>
	  <PackageReference Include="protobuf-net" />
